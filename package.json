{
  "name": "reactjstables",
  "version": "1.0.0",
  "description": "John showing that he can program JS apps",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register test/setup.js --require ignore-styles --reporter spec 'test/' --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johntran/js-challenge.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "John Tran (http://github.com/johntran)",
  "license": "CC0-1.0",
  "homepage": "https://github.com/johntran/js-challenge",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^3.0.0",
    "enzyme": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "ignore-styles": "^1.2.0",
    "jsdom": "^8.0.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "react-addons-test-utils": "^0.14.7",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.4.0",
